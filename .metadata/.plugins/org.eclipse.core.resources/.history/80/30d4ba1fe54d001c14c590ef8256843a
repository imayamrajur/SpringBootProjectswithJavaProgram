package com.raju.forloop;

import java.io.PrintStream;

public class ForLoop {
	
	static {
		System.out.println("Hi Onesoft");
	}

	public static void main(String[] args) {
		int i = 0;
		for (;;) {
			if (i < 10) {
				System.out.println(i);
				i++;
				i++;
				i++;
			} else {
				break;
			}
		}
		int j = 0;
		for (; j < 10;) {
			System.out.print("\t" + j);
			j++;
		}
		
		System.err.println("Raju R");				
	}

}

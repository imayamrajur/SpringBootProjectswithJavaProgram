package com.raju.strings;

public class TwoString {

	public static void main(String[] args) {
		String s1 = "abcdef";
		String s2 = "ghicdekl";
		String s3="";
		for(int i=0;i<s2.length();i++) {
			char c1 = s2.charAt(i);
			for(int j=0;j<s1.length();j++) {
				char c2 = s1.charAt(j);
				if(c1==c2) {
					s3 = s3+c2;
				}
			}
		}

	}

}

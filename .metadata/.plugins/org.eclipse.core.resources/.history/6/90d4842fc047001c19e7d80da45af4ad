package com.raju.strings;

public class TwoString {

	public static void main(String[] args) {
		String s1 = "cde";
		String s2 = "ghicdekl";
		String seq = "";
		String s3 = "";
		for(int i=0;i<s1.length();i++) {
			char c1 = s1.charAt(i);
			for(int j=0;j<s2.length();) {
				char c2 = s2.charAt(j);
				if(c1==c2) {
					seq = seq+c2;
					j+=j;
					break;
				}
				j++;
			}
		}
		

	}

}
